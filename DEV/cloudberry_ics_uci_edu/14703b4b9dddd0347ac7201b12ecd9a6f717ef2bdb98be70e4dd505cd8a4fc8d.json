{"url": "http://cloudberry.ics.uci.edu/apps/twittermap", "content": " \n\n\n<!DOCTYPE html>\n<html ng-app=\"cloudberry\">\n\n<head>\n  <title>TwitterMap</title>\n  <meta charset=\"UTF-8\" name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n\n  <link rel='shortcut icon' type='image/png' href='/assets/images/berry-icon.png'>\n\n  <link rel=\"stylesheet\" media=\"screen\" href='/assets/lib/bootstrap/css/bootstrap.min.css'>\n  <link rel=\"stylesheet\" media=\"screen\" href='/assets/lib/leaflet/leaflet.css'>\n  <link rel=\"stylesheet\" media=\"screen\" href='/assets/lib/dc.js/dc.min.css'>\n  <link rel=\"stylesheet\" media=\"screen\" href='/assets/lib/jquery-ui/jquery-ui.min.css'> \n  <link rel=\"stylesheet\" media=\"screen\" href='/assets/lib/font-awesome/css/font-awesome.min.css'>\n  <link rel=\"stylesheet\" media=\"screen\" href='/assets/lib/bootstrap-vertical-tabs/bootstrap.vertical-tabs.min.css'>\n  <link rel=\"stylesheet\" media=\"screen\" href='/assets/lib/bootstrap-toggle/css/bootstrap-toggle.min.css'>\n  <link rel='stylesheet' href='/assets/stylesheets/main.css'>\n\n  <script>\n    var config= {\n      sentimentEnabled: false,\n      sentimentUDF: \"twitter.`snlp#getSentimentScore`(text)\",\n      startDate: new Date(\"2015-11-22T00:00:00.000\"),\n      \n      \n      mapLegend: \"Count:\",\n      sumText : \"tweets\",\n      \n      removeSearchBar: false,\n      predefinedKeywords: [\"Diabetes\",\"Sleep Disorder\"],\n      \n      // The default caching threshold value\n      cacheThreshold: new String(\"30000\"),\n      \n      \n      // The default query slicing milliseconds value\n      querySliceMills: new String(\"2000\"),\n      \n      heatmapSamplingDayRange: 30,\n      heatmapSamplingLimit: 5000,\n      heatmapUnitIntensity: 50,\n      heatmapUnitRadius: 15,\n      pinmapSamplingDayRange: 1460,\n      pinmapSamplingLimit: 100000,\n      liveTweetQueryInterval: 30,\n      liveTweetQueryOffset: 30,\n      liveTweetUpdateRate: 6,\n      enableLiveTweet: true,\n      defaultMapType: \"countmap\"\n    }\n  </script>\n  <script src='/assets/lib/angularjs/angular.min.js'></script>\n  <script src='/assets/lib/json-bigint/dist/json-bigint.js'></script>\n  <script src='/assets/lib/leaflet/leaflet.js'></script>\n  <script src='/assets/lib/angular-leaflet-directive/angular-leaflet-directive.min.js'></script>\n  <script src='/assets/lib/jquery/jquery.min.js'></script>\n  <script src='/assets/lib/hashmap/hashmap.js'></script>\n  <script src='/assets/lib/d3/d3.min.js'></script>\n  <script src='/assets/lib/crossfilter/crossfilter.min.js'></script>\n  <script src='/assets/lib/dc.js/dc.min.js'></script>\n  <script src='/assets/lib/bootstrap/js/bootstrap.min.js'></script>\n  <script src='/assets/lib/bootstrap-toggle/js/bootstrap-toggle.min.js'></script>\n  <script src='/assets/lib/jquery-ui/jquery-ui.min.js'></script>\n  <script src='/assets/lib/chart.js/dist/Chart.bundle.min.js'></script>\n  <script src='/assets/javascripts/lib/rbush.min.js'></script>\n  <script src='/assets/javascripts/lib/turf.min.js'></script>\n  <script src='/assets/javascripts/app.js'></script>\n  <script src='/assets/javascripts/cache/services.js'></script>\n  <script src='/assets/javascripts/mapresultcache/services.js'></script>\n  <script src='/assets/javascripts/timeseriescache/services.js'></script>\n  <script src='/assets/javascripts/populationcache/services.js'></script>\n  <script src='/assets/javascripts/map/controllers.js'></script>\n  <script src='/assets/javascripts/map/countmap.js'></script>\n  <script src='/assets/javascripts/map/pinmap.js'></script>\n  <script src='/assets/javascripts/map/heatmap.js'></script>\n  <script src='/assets/javascripts/map/multilayer-manager.js'></script>\n  <script src='/assets/javascripts/lib/leaflet_canvas_layer.js'></script>\n  <script src='/assets/javascripts/lib/shader_code.js'></script>\n  <script src='/assets/javascripts/lib/webgl_point_layer.js'></script>\n  <script src='/assets/javascripts/lib/leaflet-heat.js'></script>\n\n  \n  \n      <script src='/assets/javascripts/common/services.js'></script>\n    \n  <script src='/assets/javascripts/common/module-manager.js'></script>\n  <script src='/assets/javascripts/common/cloudberry-client.js'></script>\n  <script src='/assets/javascripts/common/query-util.js'></script>\n  <script src='/assets/javascripts/common/chart-util.js'></script>\n  <script src='/assets/javascripts/timeseries/controllers.js'></script>\n  <script src='/assets/javascripts/sidebar/controllers.js'></script>\n  <script src='/assets/javascripts/searchbar/controllers.js'></script>\n  <script src='/assets/data/stopwords.js'></script>\n\n</head>\n\n<body>\n\n<div xmlns=\"http://www.w3.org/1999/html\" ng-controller=\"AppCtrl\">\n\n  <div class=\"map-group\">\n    <alert-bar></alert-bar>\n\n    <map lat=\"39.5\" lng=\"-96.35\" zoom=\"4\"></map>\n\n    <div id=\"logo\">\n    </div>\n    \n    <div id ='powerby'>\n      <div class=\"btn btn-info btn-xs\">\n        <a href=\"http://cloudberry.ics.uci.edu/\" title=\"A Distributed middleware by ICS,UCI\" class=\"white\"> Cloudberry</a>\n          |\n        <a href=\"https://www.youtube.com/watch?v=XwlRm0jcsU4\" class=\"white\">Video</a> \n      </div>\n   </div>\n\n    \n   </div>\n\n   <search-bar></search-bar>\n   <predefined-keywords></predefined-keywords>\n\n   <!-- button for hide and show the timebar -->\n  <div>\n    <button type=\"button\" class=\"btn btn-default btn-lg slide-up-down\">\n      <span id=\"downbutton\" class=\"glyphicon glyphicon-menu-down\" aria-hidden=\"true\"></span>\n    </button>\n  </div>\n\n  <div class=\"stats\">\n    <time-series id=\"chart\"></time-series>\n  </div>\n\n  <!-- button for hiding and showing the sidebar -->\n  <div ng-controller=\"SidebarCtrl\">\n    <div class=\"w3-teal\">\n      <button id=\"hamburgerButton\" type=\"button\" class=\"btn btn-default btn-sm\" ng-click=\"openRightMenu()\">\n        <span id=\"hamburger\" class=\"glyphicon glyphicon-menu-hamburger\" aria-hidden=\"true\"></span>\n      </button>\n    </div>\n\n    <div id='sidebar' class=\"open\">\n      <div class = \"col-xs-2\">\n        <a href=\"javascript:void(0)\" class=\"closebtn\" ng-click=\"closeRightMenu()\">&times;</a>\n        <ul class=\"nav nav-tabs tabs-left\">\n          <li role=\"presentation\" ng-click=\"showTab('hashtagTab')\"><a href=\"#hashtag\" id=\"Hashtag\" data-toggle=\"tab\"><i class=\"fa fa-hashtag fa-2x\" aria-hidden=\"true\"></i></a></li>\n          <li role=\"presentation\" ng-click=\"showTab('sampletweetTab')\" class=\"active\"><a href=\"#tweet\" data-toggle=\"tab\"><i class=\"fa fa-twitter fa-2x\" aria-hidden=\"true\"></i></a></li>\n          <li role=\"presentation\" ng-click=\"showTab('aboutTab')\"><a href=\"#about\" data-toggle=\"tab\"><i class=\"fa fa-info-circle fa-2x\"></i></a></li>\n          <li><br/></li>\n        </ul>\n      </div>\n      <div class=\"col-xs-10\">\n        <div class=\"tab-content\">\n          <hashtag id=\"hashtag\" class=\"tab-pane\"></hashtag>\n          <tweet id=\"tweet\" class=\"tab-pane active\"></tweet>\n          <div id=\"about\" class=\"tab-pane\">\n            <h1> About </h1>\n            <p><b>TwitterMap</b> is a research prototype powered by <a href=\"http://cloudberry.ics.uci.edu/\">Cloudberry</a>\n              and <a href=\"https://asterixdb.apache.org/\">Apache AsterixDB</a> to support\n              interactive analytics and visualization of large amounts of tweets. </p>\n            <p> Basic Information: </p>\n            <ul>\n              <li>Data set: Tweets</li>\n              <li>Number of records: > 1.3 billions </li>\n              <li>Collection period: From 2015-11-23 </li>\n              <li>Total data size: > 2TB </li>\n              <li>The live tweets is appending to db at the speed of ~20 tweets/sec </li>\n              <li><a href=\"https://github.com/ISG-ICS/cloudberry\">Source code</a></li>\n            </ul>\n            <p>Here is the small cluster where the server runs! </p>\n            <img src='/assets/images/nuc.cluster.jpg' width=\"256\" height=\"192\"><br>\n            <p>For questions and comments, please contact <b>\n                <a href=\"mailto:&#105;&#099;&#115;&#045;&#099;&#108;&#111;&#117;&#100;&#098;&#101;&#114;&#114;&#121;&#064;&#117;&#099;&#105;&#046;&#101;&#100;&#117;\">\n                  &#105;&#099;&#115;&#045;&#099;&#108;&#111;&#117;&#100;&#098;&#101;&#114;&#114;&#121;&#064;&#117;&#099;&#105;&#046;&#101;&#100;&#117;</a></b></p>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n\n  <mapchoose>\n    <ul>\n      <li role=\"presentation\"><img id=\"img1\" name=\"img1\" title=\"Count Map\" src=\"/assets/images/aggregation_map.png\" width=\"45\" height=\"45\" ></li>\n      <li role=\"presentation\"><img id=\"img2\" name=\"img2\" title=\"Heat Map\" src=\"/assets/images/heat_map_no_border.png\" width=\"45\" height=\"45\" ></li>\n      <li role=\"presentation\"><img id=\"img3\" name=\"img3\" title=\"Pin Map\" src=\"/assets/images/point_map_no_border.png\" width=\"45\" height=\"45\"></li>\n    </ul>\n  </mapchoose>\n\n  <div class = \"exception-bar\">\n    <exception-bar></exception-bar>\n  </div>\n\n</div>\n\n\n</body>\n</html>\n\n\n<script>\n  // Function for the button that hiding and showing the timebar. \n  // The button moves together with timebar and the arrow direction changes. \n  $(document).ready(function() {\n    $(\".slide-up-down\").click(function() {\n      $(\".stats\").slideToggle();\n      if( $( '#downbutton' ).hasClass( 'glyphicon-menu-down' ) ) {\n        $( '#downbutton' ).removeClass( 'glyphicon-menu-down' ).addClass('glyphicon-menu-up');\n        $(this).animate({bottom: '5px'});\n      }\n      else {\n        $( '#downbutton' ).removeClass( 'glyphicon-menu-up').addClass( 'glyphicon-menu-down');\n        $(this).animate({bottom: '110px'}); \n      }\n    });\n  });\n\n</script>\n", "encoding": "ascii"}