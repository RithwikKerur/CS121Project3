indow.addEvent('load', function() {\n\t\t\t\tnew JCaption('img.caption');\n\t\t\t});\n  </script>\n\r\n\r\n<!--[if lte IE 6]>\r\n<link href=\"/templates/beez_20/css/ieonly.css\" rel=\"stylesheet\" type=\"text/css\" />\r\n<style type=\"text/css\">\r\n#line {\r\n  width:98% ;\r\n}\r\n.logoheader {\r\n  height:200px;\r\n}\r\n#header ul.menu {\r\n  display:block !important;\r\n  width:98.2% ;\r\n}\r\n</style>\r\n<![endif]-->\r\n\r\n<!--[if IE 7]>\r\n<link href=\"/templates/beez_20/css/ie7only.css\" rel=\"stylesheet\" type=\"text/css\" />\r\n<![endif]-->\r\n<!--<script type=\"text/javascript\">\r\n  var big ='72%';\r\n  var small='53%';\r\n  var altopen='is open';\r\n  var altclose='is closed';\r\n  var bildauf='/templates/beez_20/images/plus.png';\r\n  var bildzu='/templates/beez_20/images/minus.png';\r\n  var rightopen='Open info';\r\n  var rightclose='Close info';\r\n  var fontSizeTitle='Font size';\r\n  var bigger='Bigger';\r\n  var reset='Reset';\r\n  var smaller='Smaller';\r\n  var biggerTitle='Increase size';\r\n  var resetTitle='Revert styles to default';\r\n  var smallerTitle='Decrease size';\r\n</script>-->\r\n\r\n</head>\r\n\r\n<body>\r\n\r\n<div id=\"all\">\r\n        <div id=\"back\">\r\n                <div id=\"header\">\r\n                                <div class=\"logoheader\">\r\n                                        <h1 id=\"logo\">\r\n\r\n                                                                                                                                                                                                        <span class=\"header1\">\r\n                                                                                </span></h1>\r\n                                </div><!-- end logoheader -->\r\n                                        <ul class=\"skiplinks\">\r\n                                                <li><a href=\"#main\" class=\"u2\">Skip to content</a></li>\r\n                                                <li><a href=\"#nav\" class=\"u2\">Jump to main navigation and login</a></li>\r\n                                                                                    </ul>\r\n                                        <!--<h2 class=\"unseen\">Nav view search</h2>-->\r\n                                        <h3 class=\"unseen\">Navigation</h3>\r\n                                        \n<ul class=\"menu\">\n<li class=\"item-464 active\"><a href=\"/\" >Home</a></li><li class=\"item-469\"><a href=\"/index.php/becoming-a-tutor\" >Becoming a Tutor</a></li><li class=\"item-470\"><a href=\"/index.php/tutor-resources\" >Learning Resources</a></li><li class=\"item-477\"><a href=\"/index.php/tutor-coordinators\" >Tutor Coordinators</a></li><li class=\"item-476\"><a href=\"/index.php/contact\" >Contact Us</a></li></ul>\n\r\n                                        <div id=\"line\">\r\n                                        <!-- <div id=\"fontsize\"></div> -->\r\n                                        <!--<h3 class=\"unseen\">Search</h3> -->\r\n                                        \r\n                                        </div> <!-- end line -->\r\n\r\n\r\n                        </div><!-- end header -->\r\n                        <div id=\"contentarea\">\r\n                                        <div id=\"breadcrumbs\">\r\n\r\n                                                        \r\n\r\n                                        </div>\r\n\r\n                                        \r\n\r\n                                                        <div class=\"left1 leftbigger\" id=\"nav\">\r\n                                                   <div class=\"moduletable_menu\">\n <h3><span\n\tclass=\"backh\"><span class=\"backh2\"><span class=\"backh3\">This Site</span></span></span></h3>\n \n<ul class=\"menu\">\n<li class=\"item-464 active\"><a href=\"/\" >Home</a></li><li class=\"item-469\"><a href=\"/index.php/becoming-a-tutor\" >Becoming a Tutor</a></li><li class=\"item-470\"><a href=\"/index.php/tutor-resources\" >Learning Resources</a></li><li class=\"item-477\"><a href=\"/index.php/tutor-coordinators\" >Tutor Coordinators</a></li><li class=\"item-476\"><a href=\"/index.php/contact\" >Contact Us</a></li></ul>\n</div>\n\r\n                                                                \r\n                                                                \r\n\r\n\r\n                                                        </div><!-- end navi -->\r\n               \r\n                                        <div id=\"wrapper2\" >\r\n\r\n                                                <div id=\"main\">\r\n\r\n                                                \r\n                                                        \n<div id=\"system-message-container\">\n</div>\r\n                                                        <div class=\"item-page\">\n\n\t<h2>\n\t\t\t<a href=\"/index.php/79-python-resources/105-python-try-exercises\">\n\t\tPython Try Exercises</a>\n\t\t</h2>\n\n\n\n\n\n\n\n\n<p><strong>By Aaron Hong (edits by Haoming Li)</strong></p>\r\n<p style=\"text-align: justify;\"><br />Teaching Material - Try and Except Exercises</p>\r\n<p style=\"text-align: justify;\">&lt;p&gt;</p>\r\n<p style=\"text-align: justify;\">1) Fill in the missing code chunks.</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; The following function needs to open and close a file from a directory,</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; Please fill in the correct missing code statement that would best complete</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; this funciton.</p>\r\n<p style=\"text-align: justify;\">def read_first_line(file_directory: str) -&gt; ____:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; \"\"\"takes a file directory as input, and prints the first line of text from the file.\"\"\"</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; file = _____</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; try:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp; &nbsp;file = open( __________ , ___ )</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp; &nbsp;print( _____ . ______ )</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; ______:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp; &nbsp;print(\"File not found!\")</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; finally:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp; &nbsp;if _____&nbsp; ___&nbsp; _____:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; &nbsp;file.close()</p>\r\n<p style=\"text-align: justify;\">2) What is the expected output for the following function calls?</p>\r\n<p style=\"text-align: justify;\"><br />a = 1</p>\r\n<p style=\"text-align: justify;\">b = 5</p>\r\n<p style=\"text-align: justify;\">c = 'qwerty'</p>\r\n<p style=\"text-align: justify;\"><br />def square(n: int) -&gt; int:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; result = 5</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; try:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp; &nbsp;result = (n * n)</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; except:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp; &nbsp;result += 7</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; else:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp; &nbsp;result += 5</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; finally:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp; &nbsp;try:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;&nbsp; result -= n</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp; &nbsp;except:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;&nbsp; print('error')</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; return result</p>\r\n<p style=\"text-align: justify;\"><br />What is the output of:</p>\r\n<p style=\"text-align: justify;\">print(square(a))</p>\r\n<p style=\"text-align: justify;\">print(square(b))</p>\r\n<p style=\"text-align: justify;\">print(square(c))</p>\r\n<p style=\"text-align: justify;\"><br />###################Augment_Part########################</p>\r\n<p style=\"text-align: justify;\"><br />3) What is the expected output for the following function calls?</p>\r\n<p style=\"text-align: justify;\">def tryExcept(L: list) -&gt; list:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; result = []</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; for i in L:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; try:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; result.append(i+'abc')</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; except:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; result.append(i)</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; else:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; result.append(i+'efg')</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; finally:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; try:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; result.append(i+10)</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; except:</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; result.append('hij')</p>\r\n<p style=\"text-align: justify;\">&nbsp;&nbsp;&nbsp; return result</p>\r\n<p style=\"text-align: justify;\">&lt;/pre&gt;&lt;/code&gt;</p>\r\n<p style=\"text-align: justify;\"><br />What is the output of:</p>\r\n<p style=\"text-align: justify;\"><br />print(tryExcept([5,6]))</p>\r\n<p style=\"text-align: justify;\"><br />print(tryExcept(['xyz',6]))</p>\r\n<p style=\"text-align: justify;\"><br />print(tryExcept([1,[3,2],'xyz']))</p>\r\n<p style=\"text-align: justify;\"><br />#######################################<br />&lt;/p&gt;</p>\r\n<p style=\"text-align: justify;\">Answer Key:</p>\r\n<p style=\"text-align: justify;\">1)</p>\r\n<p style=\"text-align: justify;\">None</p>\r\n<p style=\"text-align: justify;\">None</p>\r\n<p style=\"text-align: justify;\">file_directory, r</p>\r\n<p style=\"text-align: justify;\">file, readline()</p>\r\n<p style=\"text-align: justify;\">except</p>\r\n<p style=\"text-align: justify;\">file, !=, None</p>\r\n<p style=\"text-align: justify;\">2)</p>\r\n<p style=\"text-align: justify;\">5</p>\r\n<p style=\"text-align: justify;\">25</p>\r\n<p style=\"text-align: justify;\">error, 12</p>\r\n<p style=\"text-align: justify;\"><br />###################Augment_Part########################</p>\r\n<p style=\"text-align: justify;\">3)</p>\r\n<p style=\"text-align: justify;\">[5, 15, 6, 16]</p>\r\n<p style=\"text-align: justify;\">['xyzabc', 'xyzefg', 'hij', 6, 16]</p>\r\n<p style=\"text-align: justify;\">[1, 11, [3, 2], 'hij', 'xyzabc', 'xyzefg', 'hij']</p>\r\n<p style=\"text-align: justify;\">#####################################################</p> \n\t\n</div>\n\r\n\r\n                                                </div><!-- end main -->\r\n\r\n                                        </div><!-- end wrapper -->\r\n\r\n                                \r\n                        \r\n                                <div class=\"wrap\"></div>\r\n\r\n                                </div> <!-- end contentarea -->\r\n\r\n                        </div><!-- back -->\r\n\r\n                </div><!-- all -->\r\n\r\n                <div id=\"footer-outer\">\r\n              