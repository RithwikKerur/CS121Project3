ity:hidden'><p:slide coordsize=\"720,540\"\r\n colors=\"#FFFFFF,#000000,#5490A8,#003366,#0099CC,#3366CC,#99CCFF,#E1E1B7\"\r\n masterhref=\"master04.xml\">\r\n <p:shaperange href=\"master04.xml#_x0000_s2049\"/><![if !ppt]><p:shaperange\r\n  href=\"master04.xml#_x0000_s2050\"/><![if !vml]><img border=0\r\n v:shapes=\"_x0000_s2050,_x0000_s2052,_x0000_s2053,_x0000_s2054,_x0000_s2055,_x0000_s2056,_x0000_s2057,_x0000_s2058,_x0000_s2059,_x0000_s2060,_x0000_s2061,_x0000_s2062,_x0000_s2063,_x0000_s2064,_x0000_s2065,_x0000_s2066,_x0000_s2067,_x0000_s2068,_x0000_s2069,_x0000_s2070,_x0000_s2071,_x0000_s2072\"\r\n src=\"master04_image002.gif\" style='position:absolute;top:0%;left:0%;\r\n width:11.98%;height:100.5%'><![endif]><p:shaperange\r\n  href=\"master04.xml#_x0000_s2075\"/><p:shaperange\r\n  href=\"master04.xml#_x0000_s2076\"/><![endif]><p:shaperange\r\n  href=\"master04.xml#_x0000_m2073\"/><v:shape id=\"_x0000_s25602\" type=\"#_x0000_m2073\"\r\n  style='position:absolute;left:92.375pt;top:24pt;width:612pt;height:54pt'\r\n  o:userdrawn=\"f\">\r\n  <v:fill o:detectmouseclick=\"f\"/>\r\n  <v:stroke o:forcedash=\"f\"/>\r\n  <o:lock v:ext=\"edit\" text=\"f\"/>\r\n  <p:placeholder type=\"title\"/></v:shape><p:shaperange\r\n  href=\"master04.xml#_x0000_m2074\"/><v:shape id=\"_x0000_s25603\" type=\"#_x0000_m2074\"\r\n  style='position:absolute;left:96pt;top:84pt;width:596.375pt;height:438pt'\r\n  o:userdrawn=\"f\">\r\n  <v:fill o:detectmouseclick=\"f\"/>\r\n  <v:stroke o:forcedash=\"f\"/>\r\n  <o:lock v:ext=\"edit\" text=\"f\"/>\r\n  <p:placeholder type=\"body\" position=\"1\"/></v:shape>\r\n <div v:shape=\"_x0000_s25602\" class=T style='position:absolute;top:5.25%;\r\n left:13.85%;width:83.14%;height:9.25%'>1.7 Internet Standards</div>\r\n <div v:shape=\"_x0000_s25603\" class=B>\r\n <div style='mso-line-spacing:\"90 20 0\"'><span style='position:absolute;\r\n top:16.0%;left:18.16%;width:77.15%'><span style='font-size:75%'><span\r\n style='mso-special-format:bullet;color:#0099CC;mso-color-index:4;position:\r\n absolute;left:-4.85%;top:.39em;font-family:\"Monotype Sorts\";font-size:70%'>n</span></span><span\r\n style='font-size:75%'><b>The Internet is run by </b></span><span\r\n style='font-size:75%;color:#0099CC;mso-color-index:4'><b>several</b></span><span\r\n style='font-size:75%'><b> standards </b></span></span><span style='position:\r\n absolute;top:20.75%;left:18.16%;width:77.15%'><span style='font-size:75%'><b>organizations.&#13;</b></span></span></div>\r\n <div style='mso-line-spacing:\"90 20 0\"'><span style='position:absolute;\r\n top:26.75%;left:18.16%;width:86.32%'><span style='font-size:75%'><span\r\n style='mso-special-format:bullet;color:#0099CC;mso-color-index:4;position:\r\n absolute;left:-4.33%;top:.39em;font-family:\"Monotype Sorts\";font-size:70%'>n</span></span><span\r\n style='font-size:75%'><b>The Internet Engineering Task Force (</b></span><span\r\n style='font-size:75%;color:#0099CC;mso-color-index:4'><b>IETF</b></span><span\r\n style='font-size:75%'><b>) is </b></span></span><span style='position:absolute;\r\n top:31.5%;left:18.16%;width:77.15%'><span style='font-size:75%'><b>responsible\r\n for near term planning and </b></span></span><span style='position:absolute;\r\n top:36.25%;left:18.16%;width:84.26%'><span style='font-size:75%'><b>develops\r\n the specifications that become the </b></span></span><span style='position:\r\n absolute;top:41.0%;left:18.16%;width:77.15%'><span style='font-size:75%'><b>Internet\r\n standards.&#13;</b></span></span></div>\r\n <div style='mso-line-spacing:\"90 20 0\"'><span style='position:absolute;\r\n top:47.0%;left:18.16%;width:81.46%'><span style='font-size:75%'><span\r\n style='mso-special-format:bullet;color:#0099CC;mso-color-index:4;position:\r\n absolute;left:-4.59%;top:.39em;font-family:\"Monotype Sorts\";font-size:70%'>n</span></span><span\r\n style='font-size:75%'><b>The Internet Research Task Force (</b></span><span\r\n style='font-size:75%;color:#0099CC;mso-color-index:4'><b>IRTF</b></span><span\r\n style='font-size:75%'><b>) is </b></span></span><span style='position:absolute;\r\n top:51.75%;left:18.16%;width:91.01%'><span style='font-size:75%'><b>responsible\r\n for long term planning. They do not </b></span></span><span style='position:\r\n absolute;top:56.5%;left:18.16%;width:77.15%'><span style='font-size:75%'><b>produce\r\n any standards.&#13;</b></span></span></div>\r\n <div style='mso-line-spacing:\"90 20 0\"'><span style='position:absolute;\r\n top:62.5%;left:18.16%;width:89.51%'><span style='font-size:75%'><span\r\n style='mso-special-format:bullet;color:#0099CC;mso-color-index:4;position:\r\n absolute;left:-4.18%;top:.39em;font-family:\"Monotype Sorts\";font-size:70%'>n</span></span><span\r\n style='font-size:75%'><b>The Internet Architecture Board (</b></span><span\r\n style='font-size:75%;color:#0099CC;mso-color-index:4'><b>IAB</b></span><span\r\n style='font-size:75%'><b>) oversees </b></span></span><span style='position:\r\n absolute;top:67.25%;left:18.16%;width:84.26%'><span style='font-size:75%'><b>the\r\n work of the IETF and IRTF. It makes sure </b></span></span><span\r\n style='position:absolute;top:72.0%;left:18.16%;width:88.38%'><span\r\n style='font-size:75%'><b>that all the standards are coherent and correct </b></span></span><span\r\n style='position:absolute;top:76.75%;left:18.16%;width:77.15%'><span\r\n style='font-size:75%'><b>before final approval.&#13;</b></span></span></div>\r\n <div style='mso-line-spacing:\"90 20 0\"'><span style='position:absolute;\r\n top:82.5%;left:18.16%;width:86.32%'><span style='font-size:75%'><span\r\n style='mso-special-format:bullet;color:#0099CC;mso-color-index:4;position:\r\n absolute;left:-4.33%;top:.39em;font-family:\"Monotype Sorts\";font-size:70%'>n</span></span><span\r\n style='font-size:75%'><b>The Internet Society (</b></span><span\r\n style='font-size:75%;color:#0099CC;mso-color-index:4'><b>ISOC</b></span><span\r\n style='font-size:75%'><b>) is responsible for </b></span></span><span\r\n style='position:absolute;top:87.5%;left:18.16%;width:79.4%'><span\r\n style='font-size:75%'><b>the overall growth and global reach of the </b></span></span><span\r\n style='position:absolute;top:92.25%;left:18.16%;width:77.15%'><span\r\n style='font-size:75%'><b>Internet. It oversees the IAB.</b></span></span></div>\r\n </div>\r\n</p:slide></div>\r\n\r\n</body>\r\n\r\n</html>\r\n", "encoding": "ascii"}