    var output_eta = document.getElementById(\"value_eta\");\r\n    output_eta.innerHTML = slider_eta.value;\r\n\r\n    var slider_alphaT = document.getElementById(\"range_alphaT\");\r\n    var output_alphaT = document.getElementById(\"value_alphaT\");\r\n    output_alphaT.innerHTML = slider_alphaT.value;\r\n\r\n    var slider_alphaB = document.getElementById(\"range_alphaB\");\r\n    var output_alphaB = document.getElementById(\"value_alphaB\");\r\n    output_alphaB.innerHTML = slider_alphaB.value;\r\n\r\n    var slider_sigmaT = document.getElementById(\"range_sigmaT\");\r\n    var output_sigmaT = document.getElementById(\"value_sigmaT\");\r\n    output_sigmaT.innerHTML = slider_sigmaT.value;\r\n\r\n    var slider_g = document.getElementById(\"range_g\");\r\n    var output_g = document.getElementById(\"value_g\");\r\n    output_g.innerHTML = slider_g.value;\r\n\r\n    slider_eta.oninput = function() {\r\n      eta_this = this.value;\r\n      output_eta.innerHTML = eta_this;\r\n      img.src = \"dataset1/\" + eta_this + \"_\" + alphaT_this + \"_\" + sigmaT_this + \"_\" + g_this + \"_\" + alphaB_this + \".png\";    \r\n    }\r\n\r\n    slider_alphaT.oninput = function() {\r\n      alphaT_this = this.value;\r\n      output_alphaT.innerHTML = alphaT_this;\r\n      img.src = \"dataset1/\" + eta_this + \"_\" + alphaT_this + \"_\" + sigmaT_this + \"_\" + g_this + \"_\" + alphaB_this + \".png\";\r\n    }\r\n\r\n    slider_alphaB.oninput = function() {\r\n      alphaB_this = this.value;\r\n      output_alphaB.innerHTML = alphaB_this;\r\n      img.src = \"dataset1/\" + eta_this + \"_\" + alphaT_this + \"_\" + sigmaT_this + \"_\" + g_this + \"_\" + alphaB_this + \".png\";    \r\n    }\r\n\r\n    slider_sigmaT.oninput = function() {\r\n      sigmaT_this = this.value;\r\n      output_sigmaT.innerHTML = sigmaT_this;\r\n      img.src = \"dataset1/\" + eta_this + \"_\" + alphaT_this + \"_\" + sigmaT_this + \"_\" + g_this + \"_\" + alphaB_this + \".png\";    \r\n    }\r\n\r\n    slider_g.oninput = function() {\r\n      g_this = this.value;\r\n      output_g.innerHTML = g_this;\r\n      img.src = \"dataset1/\" + eta_this + \"_\" + alphaT_this + \"_\" + sigmaT_this + \"_\" + g_this + \"_\" + alphaB_this + \".png\";    \r\n    }\r\n\r\n    var img_2 = document.getElementById(\"img_2\");\r\n    img_2.src = \"dataset1/1.1_0.01_0.6_0_0.01.png\";\r\n\r\n    var eta_this_2 = 1.1;\r\n    var alphaT_this_2 = 0.01; \r\n    var alphaB_this_2 = 0.01;\r\n    var sigmaT_this_2 = 0.6;\r\n    var g_this_2 = 0;\r\n\r\n    var slider_eta_2 = document.getElementById(\"range_eta_2\");\r\n    var output_eta_2 = document.getElementById(\"value_eta_2\");\r\n    output_eta_2.innerHTML = slider_eta_2.value;\r\n\r\n    var slider_alphaT_2 = document.getElementById(\"range_alphaT_2\");\r\n    var output_alphaT_2 = document.getElementById(\"value_alphaT_2\");\r\n    output_alphaT_2.innerHTML = slider_alphaT_2.value;\r\n\r\n    var slider_alphaB_2 = document.getElementById(\"range_alphaB_2\");\r\n    var output_alphaB_2 = document.getElementById(\"value_alphaB_2\");\r\n    output_alphaB_2.innerHTML = slider_alphaB_2.value;\r\n\r\n    var slider_sigmaT_2 = document.getElementById(\"range_sigmaT_2\");\r\n    var output_sigmaT_2 = document.getElementById(\"value_sigmaT_2\");\r\n    output_sigmaT_2.innerHTML = slider_sigmaT_2.value;\r\n\r\n    var slider_g_2 = document.getElementById(\"range_g_2\");\r\n    var output_g_2 = document.getElementById(\"value_g_2\");\r\n    output_g_2.innerHTML = slider_g_2.value;\r\n\r\n    slider_eta_2.oninput = function() {\r\n      eta_this_2 = this.value;\r\n      output_eta_2.innerHTML = eta_this_2;\r\n      img_2.src = \"dataset1/\" + eta_this_2 + \"_\" + alphaT_this_2 + \"_\" + sigmaT_this_2 + \"_\" + g_this_2 + \"_\" + alphaB_this_2 + \".png\";    \r\n    }\r\n\r\n    slider_alphaT_2.oninput = function() {\r\n      alphaT_this_2 = this.value;\r\n      output_alphaT_2.innerHTML = alphaT_this_2;\r\n      img_2.src = \"dataset1/\" + eta_this_2 + \"_\" + alphaT_this_2 + \"_\" + sigmaT_this_2 + \"_\" + g_this_2 + \"_\" + alphaB_this_2 + \".png\";\r\n    }\r\n\r\n    slider_alphaB_2.oninput = function() {\r\n      alphaB_this_2 = this.value;\r\n      output_alphaB_2.innerHTML = alphaB_this_2;\r\n      img_2.src = \"dataset1/\" + eta_this_2 + \"_\" + alphaT_this_2 + \"_\" + sigmaT_this_2 + \"_\" + g_this_2 + \"_\" + alphaB_this_2 + \".png\";    \r\n    }\r\n\r\n    slider_sigmaT_2.oninput = function() {\r\n      sigmaT_this_2 = this.value;\r\n      output_sigmaT_2.innerHTML = sigmaT_this_2;\r\n      img_2.src = \"dataset1/\" + eta_this_2 + \"_\" + alphaT_this_2 + \"_\" + sigmaT_this_2 + \"_\" + g_this_2 + \"_\" + alphaB_this_2 + \".png\";    \r\n    }\r\n\r\n    slider_g_2.oninput = function() {\r\n      g_this_2 = this.value;\r\n      output_g_2.innerHTML = g_this_2;\r\n      img_2.src = \"dataset1/\" + eta_this_2 + \"_\" + alphaT_this_2 + \"_\" + sigmaT_this_2 + \"_\" + g_this_2 + \"_\" + alphaB_this_2 + \".png\";    \r\n    }\r\n\r\n    var aniso_img = document.getElementById(\"aniso_img\");\r\n    aniso_img.src = \"dataset2/0.01_0.01_0.01_0.01_0_0.01.png\";\r\n\r\n    var aniso_alpha1_this = 0.01;\r\n    var aniso_alpha2_this = 0.01; \r\n    var aniso_alpha3_this = 0.01;\r\n    var aniso_alpha4_this = 0.01; \r\n    var aniso_ori_this = 0;\r\n    var aniso_stddev_this = 0.01;\r\n\r\n    var slider_aniso_alpha1 = document.getElementById(\"range_aniso_alpha1\");\r\n    var output_aniso_alpha1 = document.getElementById(\"value_aniso_alpha1\");\r\n    output_aniso_alpha1.innerHTML = slider_aniso_alpha1.value;\r\n\r\n    var slider_aniso_alpha2 = document.getElementById(\"range_aniso_alpha2\");\r\n    var output_aniso_alpha2 = document.getElementById(\"value_aniso_alpha2\");\r\n    output_aniso_alpha2.innerHTML = slider_aniso_alpha2.value;\r\n\r\n    var slider_aniso_alpha3 = document.getElementById(\"range_aniso_alpha3\");\r\n    var output_aniso_alpha3 = document.getElementById(\"value_aniso_alpha3\");\r\n